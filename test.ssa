# files.

section ".rodata"
data $msg = { b "Hello, world! Number = %zu", b 10, b 0 }

export function w $main(l %argc, l %argv) {
@start
@loop
        %x =w phi @start 100, @loop %x1
        %x =w copy 1
        %x1 =w sub %x, 1
        jnz %x1, @loop, @end
@end
        ret 0
}
